        <div class="contain-search">
            <input class="form-control" type="text" placeholder="Tìm kiếm ...">
        </div>
        <div class="contain-condition">
            <div class="dropdown">
                <button class="btn btn-secondary dropdown-toggle" type="button" id="drop-condition" data-bs-toggle="dropdown" aria-expanded="false">
                    Lọc theo
                </button>
                <ul class="dropdown-menu" aria-labelledby="drop-condition">
                    <li><a class="dropdown-item" href="#">Phòng ban</a></li>
                </ul>
            </div>
        </div>
    </div>
</div>

